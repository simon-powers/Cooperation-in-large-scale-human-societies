    priority = {2},
    publisher = {Public Library of Science},
    title = {The Demographic Benefits of Belligerence and Bravery: {Defeated} Group Repopulation or Victorious Group Size Expansion?},
    url = {http://dx.doi.org/10.1371/journal.pone.0021437},
    volume = {6},
