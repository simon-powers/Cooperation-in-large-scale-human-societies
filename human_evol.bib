    journal = {Journal of Evolutionary Biology},
    keywords = {*file-import-15-09-29},
    month = sep,
    number = {9},
    pages = {1765--1774},
    posted-at = {2015-09-29 09:27:56},
    priority = {2},
    title = {{Partner switching can favour cooperation in a biological market}},
    url = {http://dx.doi.org/10.1111/jeb.12455},
    volume = {27},
