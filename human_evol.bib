    pmid = {20333237},
    posted-at = {2012-07-16 12:08:06},
    priority = {0},
    publisher = {Public Library of Science},
    title = {Emergence of Spatial Structure in Cell Groups and the Evolution of Cooperation},
    url = {http://dx.doi.org/10.1371/journal.pcbi.1000716},
