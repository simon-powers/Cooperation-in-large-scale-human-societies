    citeulike-linkout-0 = {http://dx.doi.org/10.1016/j.tpb.2013.01.003},
    doi = {10.1016/j.tpb.2013.01.003},
    issn = {00405809},
    journal = {Theoretical Population Biology},
    keywords = {cooperation, dispersal, social\_niche\_construction},
    month = may,
    pages = {12--19},
    posted-at = {2013-04-17 16:38:26},
    priority = {2},
    title = {Joint evolution of altruistic cooperation and dispersal in a metapopulation of small local populations},
    url = {http://dx.doi.org/10.1016/j.tpb.2013.01.003},
    volume = {85},
    year = {2013}
}

