    priority = {2},
    title = {Tradition over trend: Neighboring chimpanzee communities maintain differences in cultural behavior despite frequent immigration of adult females: Neighboring Chimpanzee Communities},
    url = {http://dx.doi.org/10.1002/ajp.22259},
    volume = {76},
