@article{Szamado:2008:a,
    abstract = {The prisoner's dilemma (PD) and the snowdrift (SD) games are paradigmatic tools to investigate the origin of cooperation. Whereas spatial structure (e.g. nonrandom spatial distribution of strategies) present in the spatially explicit models facilitates the emergence of cooperation in the PD game, recent investigations have suggested that spatial structure can be unfavourable for cooperation in the SD game. The frequency of cooperators in a spatially explicit SD game can be lower than it would be in an infinitely large well-mixed population. However, the source of this effect cannot be identified with certainty as spatially explicit games differ from well-mixed games in two aspects: (i) they introduce spatial correlations, (ii) and limited neighbourhood. Here we extend earlier investigations to identify the source of this effect, and thus accordingly we study a spatially explicit version of the PD and SD games with varying degrees of dispersal and neighbourhood size. It was found that dispersal favours selfish individuals in both games. We calculated the frequency of cooperators at strong dispersal limit, which in concordance with the numerical results shows that it is the short range of interactions (i.e. limited neighbourhood) and not spatial correlations that decreases the frequency of cooperators in spatially explicit models of populations. Our results demonstrate that spatial correlations are always beneficial to cooperators in both the PD and SD games. We explain the opposite effect of dispersal and neighbourhood structure, and discuss the relevance of distinguishing the two effects in general.},
    author = {Sz\'{a}mad\'{o}, Szabolcs and Szalai, Ferenc and Scheuring, Istv\'{a}n},
    citeulike-article-id = {7614617},
    citeulike-linkout-0 = {http://dx.doi.org/10.1016/j.jtbi.2008.02.037},
    day = {21},
    doi = {10.1016/j.jtbi.2008.02.037},
    issn = {00225193},
    journal = {Journal of Theoretical Biology},
    keywords = {cooperation, prisoners\_dilemma, snowdrift, spatial\_correlations, spatial\_model, spatial\_structure},
    month = jul,
    number = {2},
    pages = {221--227},
    posted-at = {2010-08-11 12:30:11},
    priority = {5},
    title = {The effect of dispersal and neighbourhood in games of cooperation},
    url = {http://dx.doi.org/10.1016/j.jtbi.2008.02.037},
    volume = {253},
    year = {2008}
}

@article{King:2004:a,
    abstract = {The transition to multicellularity that launched the evolution of animals from protozoa marks one of the most pivotal, and poorly understood, events in life's history. Advances in phylogenetics and comparative genomics, and particularly the study of choanoflagellates, are yielding new insights into the biology of the unicellular progenitors of animals. Signaling and adhesion gene families critical for animal development (including receptor tyrosine kinases and cadherins) evolved in protozoa before the origin of animals. Innovations in transcriptional regulation and expansions of certain gene families may have allowed the integration of cell behavior during the earliest experiments with multicellularity. The protozoan perspective on animal origins promises to provide a valuable window into the distant past and into the cellular bases of animal development.},
