    keywords = {group\_selection, superorganism},
    month = feb,
    number = {3},
    pages = {337--356},
    posted-at = {2011-03-12 22:04:55},
    priority = {2},
    title = {Reviving the superorganism},
    url = {http://dx.doi.org/10.1016/s0022-5193(89)80169-9},
    volume = {136},
