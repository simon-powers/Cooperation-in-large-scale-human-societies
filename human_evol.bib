    doi = {10.1371/journal.pcbi.1002113},
    editor = {Traulsen, Arne},
    issn = {1553-7358},
    journal = {PLoS Computational Biology},
    keywords = {group\_size, indirect\_recprocity},
    month = jul,
    number = {7},
    pages = {e1002113+},
    posted-at = {2014-04-01 09:23:11},
