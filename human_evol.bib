    month = feb,
    number = {1},
    pages = {69--80},
    posted-at = {2012-11-02 09:08:03},
    priority = {0},
    title = {A Simulation of Wright's Shifting-Balance Process: Migration and the Three Phases},
    url = {http://www.jstor.org/stable/10.1086/375299},
